---
// src/components/Logo.astro
const strokeW = "13";
const strokeC = "#FFF";
// const starF = "rgba(255,255,255,0.088)";
const starF = "transparent";
//const starF = "rgba(255,255,255,0.15)";
---

<style>
  .logo.svg_wrap {
    margin-block: var(--space_block);
    padding-inline: var(--space_inline);
    min-height: 120px;
    height: clamp(120px, 120px, 300px);
  }
  /*svg {
    min-height: 200px;
    height: clamp(200px, 15dvh, 400px);
  }*/

  /*@media (min-width: 510px) {
    svg {
      max-height: clamp(200px, 15dvh, 400px);
    }
  }*/
</style>

<div class="logo svg_wrap">
  <svg
    class="jeistar"
    version="1.1"
    xmlns="http://www.w3.org/2000/svg"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    x="0px"
    y="0px"
    viewBox="-10 -10 1020 1020"
    id="jeistarlogo"
  >
    <path
      class="circlej"
      data-start="1"
      data-duration="80"
      fill="none"
      stroke="hsla(209, 100%, 70%, 1)"
      stroke-width={strokeW}
      stroke-linecap="round"
      stroke-linejoin="round"
      stroke-miterlimit="10"
      d=" M839,132.8V564c0,115.3-30.5,200-91.2,254c-60.8,54-141.2,81.8-241.2,82.8c-102.2,1-184.2-23.8-246-74.5 C198.8,775.3,168,710,168,630c0-35.2,17.2-53,51.5-53.5c36.5-0.2,54.8,13,54.8,40c0,59.2,20.8,105.8,62,139.5 c41.2,33.8,97,50.5,167,49.7c73-0.8,129.5-21,169.2-61c40-39.8,60-99.8,60-179.8V57.5C663,20.8,584,0,500,0C224.2,0,0,224.2,0,500 s224.2,500,500,500s500-224.2,500-500C1000,355,938,224.2,839,132.8z"
    ></path>

    <path
      class="circlej"
      data-start="1"
      data-duration="80"
      fill="none"
      stroke={strokeC}
      stroke-width={strokeW}
      stroke-linecap="round"
      stroke-linejoin="round"
      stroke-miterlimit="10"
      d=" M839,132.8V564c0,115.3-30.5,200-91.2,254c-60.8,54-141.2,81.8-241.2,82.8c-102.2,1-184.2-23.8-246-74.5 C198.8,775.3,168,710,168,630c0-35.2,17.2-53,51.5-53.5c36.5-0.2,54.8,13,54.8,40c0,59.2,20.8,105.8,62,139.5 c41.2,33.8,97,50.5,167,49.7c73-0.8,129.5-21,169.2-61c40-39.8,60-99.8,60-179.8V57.5C663,20.8,584,0,500,0C224.2,0,0,224.2,0,500 s224.2,500,500,500s500-224.2,500-500C1000,355,938,224.2,839,132.8z"
    ></path>

    <polygon
      class="star"
      data-start="1"
      data-delay="20"
      data-duration="50"
      fill={starF}
      stroke="hsla(209, 100%, 70%, 1)"
      stroke-width={strokeW}
      stroke-linecap="round"
      stroke-linejoin="round"
      stroke-miterlimit="10"
      points=" 122.5,459.8 260.2,328.5 227.5,143 397.8,230.2 568,142.5 535.8,327.8 673.5,459 483.2,486.2 398.2,655 313,486.5 "
    ></polygon>

    <polygon
      class="star"
      data-start="1"
      data-delay="20"
      data-duration="50"
      fill="none"
      stroke={strokeC}
      stroke-width={strokeW}
      stroke-linecap="round"
      stroke-linejoin="round"
      stroke-miterlimit="10"
      points=" 122.5,459.8 260.2,328.5 227.5,143 397.8,230.2 568,142.5 535.8,327.8 673.5,459 483.2,486.2 398.2,655 313,486.5 "
    ></polygon>
  </svg>
</div>
