  <div class="offer_content light_bg google_my_business">
    <div class="mean_title mean_title--first">
      <div class="wrap flex flex_cols flex_align_center flex_justify_center">
        <div class="mean_icon">
          <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 400 400" class="white_color outlined icon--location">
            <path
              d="M200 33.334C135.5 33.334 83.333 85.5 83.333 150 83.333 237.5 200 366.666 200 366.666S316.667 237.5 316.667 150C316.667 85.5 264.5 33.334 200 33.334zm0 158.332c-23 0-41.667-18.666-41.667-41.666S177 108.334 200 108.334 241.666 127 241.666 150 223 191.666 200 191.666z"
